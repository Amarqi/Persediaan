	<queryString>
		<![CDATA[SELECT
     barang.`barcode` AS barang_barcode,
     barang.`nama_barang` AS barang_nama_barang,
     barang.`jenis_brg` AS barang_jenis_brg,
     returpem.`no_returpem` AS returpem_no_returpem,
     returpem.`no_masuk` AS returpem_no_masuk,
     returpem.`ket_returpem` AS returpem_ket_returpem,
     returpem.`tgl_returpem` AS returpem_tgl_returpem,
     returpem.`total_returpem` AS returpem_total_returpem,
     detail_returpem.`no_returpem` AS detail_returpem_no_returpem,
     detail_returpem.`barcode` AS detail_returpem_barcode,
     detail_returpem.`jml_returpem` AS detail_returpem_jml_returpem,
     detail_returpem.`harga_returpem` AS detail_returpem_harga_returpem,
     barang_msk.`no_masuk` AS barang_msk_no_masuk,
     barang_msk.`id_user` AS barang_msk_id_user,
     barang_msk.`kode_supp` AS barang_msk_kode_supp,
     barang_msk.`tgl_msk` AS barang_msk_tgl_msk,
     barang_msk.`total_msk` AS barang_msk_total_msk,
     supplier.`kode_supp` AS supplier_kode_supp,
     supplier.`nama_supp` AS supplier_nama_supp,
     supplier.`alamat` AS supplier_alamat,
     supplier.`telp` AS supplier_telp
FROM
     `returpem` returpem INNER JOIN `detail_returpem` detail_returpem ON returpem.`no_returpem` = detail_returpem.`no_returpem`
     INNER JOIN `barang` barang ON detail_returpem.`barcode` = barang.`barcode`
     INNER JOIN `barang_msk` barang_msk ON returpem.`no_masuk` = barang_msk.`no_masuk`
     INNER JOIN `supplier` supplier ON barang_msk.`kode_supp` = supplier.`kode_supp`
WHERE
returpem.`no_returpem`=$P{trx}]]>
