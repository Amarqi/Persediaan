	<queryString>
		<![CDATA[SELECT
     barang.`barcode` AS barang_barcode,
     barang.`nama_barang` AS barang_nama_barang,
     barang.`jenis_brg` AS barang_jenis_brg,
     barang_klr.`no_keluar` AS barang_klr_no_keluar,
     barang_klr.`id_user` AS barang_klr_id_user,
     barang_klr.`tgl_klr` AS barang_klr_tgl_klr,
     barang_klr.`total_klr` AS barang_klr_total_klr,
     detail_klr.`no_keluar` AS detail_klr_no_keluar,
     detail_klr.`barcode` AS detail_klr_barcode,
     detail_klr.`jml_klr` AS detail_klr_jml_klr,
     detail_klr.`harga_klr` AS detail_klr_harga_klr,
     user.`id_user` AS user_id_user,
     user.`nm_user` AS user_nm_user,
     user.`hak_akses` AS user_hak_akses,
     user.`pass` AS user_pass,
     date_format(barang_klr.`tgl_klr`,'%M') AS bulan
FROM
     `barang_klr` barang_klr INNER JOIN `detail_klr` detail_klr ON barang_klr.`no_keluar` = detail_klr.`no_keluar`
     INNER JOIN `barang` barang ON detail_klr.`barcode` = barang.`barcode`
     INNER JOIN `user` user ON barang_klr.`id_user` = user.`id_user`
WHERE
month(tgl_klr)=$P{bln} and year(tgl_klr)=$P{thn}]]>
	</queryString>
