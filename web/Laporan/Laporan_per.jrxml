	<queryString language="SQL">
		<![CDATA[SELECT
     barang.`barcode` AS barang_barcode,
     barang.`nama_barang` AS barang_nama_barang,
     barang.`jenis_brg` AS barang_jenis_brg,
     gudang.`id_gudang` AS gudang_id_gudang,
     gudang.`barcode` AS gudang_barcode,
     gudang.`stok` AS gudang_stok,
     gudang.`hpp` AS gudang_hpp,
