	<queryString>
		<![CDATA[SELECT
     pny_barang.`no_pny` AS pny_barang_no_pny,
     pny_barang.`barcode` AS pny_barang_barcode,
     pny_barang.`tgl_pny` AS pny_barang_tgl_pny,
     pny_barang.`ket_pny` AS pny_barang_ket_pny,
     pny_barang.`harga_pny` AS pny_barang_harga_pny,
     pny_barang.`jml_pny` AS pny_barang_jml_pny,
     pny_barang.`total_pny` AS pny_barang_total_pny,
     barang.`barcode` AS barang_barcode,
     barang.`nama_barang` AS barang_nama_barang,
     barang.`jenis_brg` AS barang_jenis_brg,
     date_format(pny_barang.`tgl_pny`,'%M') AS bulan
FROM
     `barang` barang INNER JOIN `pny_barang` pny_barang ON barang.`barcode` = pny_barang.`barcode`
WHERE
month(tgl_pny)=$P{bln} and year(tgl_pny)=$P{thn}]]>
