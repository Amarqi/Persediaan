	<queryString>
		<![CDATA[SELECT
     detail_returpnj.`no_returpnj` AS detail_returpnj_no_returpnj,
     detail_returpnj.`barcode` AS detail_returpnj_barcode,
     detail_returpnj.`jml_returpnj` AS detail_returpnj_jml_returpnj,
     detail_returpnj.`harga_returpnj` AS detail_returpnj_harga_returpnj,
     returpnj.`no_returpnj` AS returpnj_no_returpnj,
     returpnj.`no_keluar` AS returpnj_no_keluar,
     returpnj.`ket_returpnj` AS returpnj_ket_returpnj,
     returpnj.`tgl_returpnj` AS returpnj_tgl_returpnj,
     returpnj.`total_returpnj` AS returpnj_total_returpnj,
     barang_klr.`no_keluar` AS barang_klr_no_keluar,
     barang_klr.`id_user` AS barang_klr_id_user,
     barang_klr.`tgl_klr` AS barang_klr_tgl_klr,
     barang_klr.`total_klr` AS barang_klr_total_klr,
     barang.`barcode` AS barang_barcode,
     barang.`nama_barang` AS barang_nama_barang,
     barang.`jenis_brg` AS barang_jenis_brg
FROM
     `returpnj` returpnj INNER JOIN `detail_returpnj` detail_returpnj ON returpnj.`no_returpnj` = detail_returpnj.`no_returpnj`
     INNER JOIN `barang_klr` barang_klr ON returpnj.`no_keluar` = barang_klr.`no_keluar`
     INNER JOIN `barang` barang ON detail_returpnj.`barcode` = barang.`barcode`
WHERE
returpnj.`no_returpnj`=$P{trx}]]>
	</queryString>
